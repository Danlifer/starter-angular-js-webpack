const { config } = require("webpack");

(function(){
    'use strict'
    angular
        .module('appGit')
        .config('config',config);

    config.$inject=['$routeProvider']
    
    function config($routeProvider){
        $routeProvider
            .when('/',{
                templateUrl:'./index.html',
                controller:'gitController',
                controllerAs:'gitCtlr'
                })
                .otherwise({redirectTo: '/'})
    }

})();